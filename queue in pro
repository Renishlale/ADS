 
#include <stdio.h>


    int max;
    printf("\n Enter the size of  the queue:");
    scanf("%d",&max);
    int queue[max];
    int front=-1;
    int rear=-1;
    
    insert(int value)
    {
        if(rear==max-1)
        {
            printf("\n Queue overflow.");
            
        }
        else
        {
            if(front==-1)
            {
                front=0;
                queue[rear++]=value;
                pritf("Inserted %d",value);
                }
            }
            
        }
    }
    int delete()
    {
        if(front=-1||front>rear)
        {
            printf("Stack underflow.");
            
        }
        else
        {
           printf("%d deleted\n ",queue[front++]); 
        }
    }
     int display()
    {
        if(front=-1||front>rear)
        {
            printf("Stack empty.");
        else
        {
            for(i=1;i<=rear;i++)
            {
                printf("%d"queue[i]);
            }
            
        }
    
    int main()
    {
        insert(10);
        insert(20);
        delete();
        insert(30);
        delete();
        delete();
        delete();
        display();
         retrun 0;
    }
    
   
    

    return 0;
}
