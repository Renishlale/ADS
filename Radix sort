#include <stdio.h>

int main()

{

int a[8];

int digit;


int i;

int max;

for(i=0;i<8;i++)

{

printf("\n Enter a[%d]=",i);

scanf("%d",&a[i]);

}

max=a[0];

for(i=0;i<8;i++)
{

if(max<a[i])

  {

    max=a[i];

  }
}



printf("Maxium number is: %d\n ", max);

 
for(int exp=1;(max/exp)>0;exp=exp*10)
{
    int count[10]={0};
    int output[8];
    
    for(i=0;i<8;i++)
    {
        int digit=(a[i]/exp)%10;
        count[digit]++;
        
    }
    for(i=1;i<10;i++)
    {
        count[i]=count[i]+(count[i-1]);
    }
    for(i=7;i>=0;i--)
    {
        digit=(a[i]/exp)%10;
        output[count[digit]-1]=a[i];
        count[digit]--;
    }  
    for(i=0;i<8;i++)
    {
       a[i]=output[i];        
    }
}    
    
    for(i=0;i<8;i++)
    {
        printf(" %d ",a[i]);        
    }
    return 0;
    
}
