#include <stdio.h>

int main()
{
    int i,j,key;
    int a[6]={7,4,10,8,3,1};
    
    for(i=1;i<6;i++)
    {
        key=a[i];
        
        j=i-1;
        while(j>=0&&a[j]>key) 
        {
            a[j+1]=a[j];
            j--;
        }
        a[j+1]=key;
    }
    printf("SORTED ORDER:");
    for(i=0;i<6;i++)
    {
        printf(" %d ",a[i]);
    }
     
     return 0;
}
