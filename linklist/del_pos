      
#include <stdio.h>
#include<stdlib.h>

struct node
{
    int data;
    struct node*next;
};

    int del_pos(struct node**head,int key)
    {
         if(*head==NULL)
        {
            printf("\n No list exist.");
        }
         struct node*temp=*head;
        if(temp->data==key&&temp==NULL)
        {
            *head=temp->next;
            free(temp);
            return 0;
        }
           struct node*prev=NULL;
        while(temp!=NULL&&temp->data!=key)
        { 
             prev=temp;
             temp=temp->next;
        }
        if(temp==NULL)
        {
            printf("\n KEY not found.");
            
        }
          printf("Delete %d \n ",temp->data);
          prev->next=temp->next;
          free(temp);
          return 0;
    }
int insert_beginning(int value,struct node**head)
{
    struct node*newnode;
    newnode=malloc(sizeof(struct node));
    newnode->data=value;
    newnode->next=*head;
    *head=newnode;
    return 0;
}

int printlist(struct node*head)
{
    struct node*temp=head;
    printf("LINK LIST:");
    
    while(temp!=NULL)
    {
        printf("%d ->",temp->data);
        temp=temp->next;
        
    }
    printf("NULL");
    
    
    
    
    
}

int main()
{
    struct node*head=NULL;
    insert_beginning(10,&head);
    insert_beginning(20,&head);
    insert_beginning(30,&head);
    insert_beginning(40,&head); 
    del_pos(&head,20);
    printlist(head);
    return 0;
    
    
    
    
}
