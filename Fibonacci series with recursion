   
#include <stdio.h>

int fib();


int main() 
{
    int n;
    
    printf("\n Enter the number of terms:");
    scanf("%d",&n);
     printf("\nFibonacii series:\n");
    for(int i=0;i<n;i++)
    {
        printf("%d",fib(i));
            printf("\n");
    }
    printf("\n");
 
    return 0;
}

int fib(int n)
{
    if(n==0)
    {
        return 0;
    }
    else if(n==1)
    {
        return 1;
    }
    else
    {
        return (fib(n-1)+fib(n-2));
    }

    
}
