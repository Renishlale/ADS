 #include <stdio.h>
 #define size 6
 int  n,top=-1 ;
 int stack[size];
 int value;
 

int ins(int value)
{
   
    if(top==size-1)
    {
        printf("Stack is full");
        return 0;
        
    }
    
    
    else
   {
     
          stack[++top]=value;
         
          printf("\n %d pushed into the stack\n ",value);
          return 0;
     
     
      
   } 
    
}
int dis()
{
    if(top==-1)
    {
        printf("Stack is empty.");
        return 0;
    }
    else{
       printf("\n -------------Stack--------------");
       for(int i=top;i>=0;i--)
       {
         printf("\n %d",stack[i]);
         
       }
       return 0;
    }
}






int main()
{
    ins(3);
    ins(20);
    
    dis();
    return 0;
}
