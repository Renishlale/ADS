  #include <stdio.h>
 #define size 6
 int  n,top=-1 ;
 int stack[size];
 int value;
 

int ins(int value)
{
   
    if(top==size-1)
    {
        printf("Stack is full");
        return 0;
        
    }
    
    
    else
   {
     
          stack[++top]=value;
         
           printf("%d pushed into the stack\n", value);
          return 0;
     
} 
}

 int del()
   {
         if (top == -1)
         {
             
          printf("Stack is empty, cannot pop element\n");
          return 0;
         }
          else
        {
          printf("\nElement popped: %d\n", stack[top]);
           top--;
           return 0;
        }
   }
int dis()
{
    if(top==-1)
    {
        printf("Stack is empty.");
        return 0;
    }
    else{
       printf("\n -------------Stack--------------");
       for(int i=top;i>=0;i--)
       {
         printf("\n %d",stack[i]);
         
       }
       return 0;
    }
}






int main()
{
    ins(3);
    ins(20);
    del();
    dis();
    return 0;
}
